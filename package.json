{
	"name": "sveltekit-1.0",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"build:fixed": "pnpm build && node ipfs_fixes/inject_base.cjs && node ipfs_fixes/inject_base_in_singletons_file.cjs && node ipfs_fixes/inject_base_in_paths_file.cjs && node ipfs_fixes/relativize_pages.cjs && node ipfs_fixes/relativize_js.cjs",
		"serve": "ipfs-emulator --only -d build -p 8080",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@fontsource/fira-mono": "^4.5.10",
		"@neoconfetti/svelte": "^1.0.0",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-static": "^2.0.1",
		"@sveltejs/kit": "^1.5.0",
		"@types/cookie": "^0.5.1",
		"ipfs-gateway-emulator": "4.2.1-ipfs.2",
		"mdsvex": "^0.10.6",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0",
		"vitest": "^0.25.3"
	},
	"type": "module"
}
